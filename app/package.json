{
  "name": "shopping-agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prep": "mkdir public/images; npm run prep-bootstrap-icons",
    "prep-bootstrap-icons": "cp -f node_modules/bootstrap-icons/bootstrap-icons.svg public/images/",
    "predev": "npm run prep",
    "dev": "DEBUG=shopping-agent:*,ebay-client:* next dev",
    "preinstall": "cd ../packages/ebay-client && npm i && npm run build",
    "gen-sitemap": "cd ./src/scripts && npx ts-node gen-sitemap.ts",
    "prebuild": "npm run prep && npm run gen-sitemap",
    "build": "next build",
    "start": "DEBUG=shopping-agent:*,ebay-client:* next start",
    "lint-next": "next lint",
    "eslint": "eslint . --ext ts,tsx,js,jsx",
    "lint": "prettier -l \"{,!(node_modules)/**/}*.{ts,tsx,md,yml,json,html}\" && npm run eslint",
    "lint-fix": "prettier --write \"{,!(node_modules)/**/}*.{ts,tsx,md,yml,json,html}\" && npm run eslint -- --fix",
    "prepare": "npm run prisma-generate && cd .. && husky install app/.husky",
    "watch": "DEBUG=shopping-agent:* jest --watch",
    "test": "DEBUG=shopping-agent:* jest",
    "prisma-validate": "npx prisma validate",
    "//prisma-generate": "generates the types for the prisma client",
    "prisma-generate": "npx prisma generate",
    "prisma-dev": "npx prisma migrate dev",
    "prisma-reset": "npx prisma migrate reset",
    "prisma-nuke": "npm run prisma-validate && rm -rf prisma/migrations && npm run prisma-dev && npm run prisma-reset",
    "prisma-seed": "npx prisma db seed"
  },
  "prisma": {
    "//seed": "seeds the database with bootstrap or test data. See https://www.prisma.io/docs/guides/migrate/seed-database.",
    "seed": "node prisma/seed.mjs"
  },
  "dependencies": {
    "@activescott/diag": "^2.0.2",
    "@mdx-js/loader": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "@next/mdx": "^14.0.2",
    "@popperjs/core": "^2.11.8",
    "@prisma/client": "^5.6.0",
    "@types/lodash": "^4.14.202",
    "@types/mdx": "^2.0.10",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.1",
    "common-tags": "^1.8.2",
    "ebay-client": "file:../packages/ebay-client",
    "glob": "^10.3.10",
    "irritable-iterable": "^1.4.0",
    "lodash": "^4.17.21",
    "next": "^14.2.20",
    "posthog-js": "^1.93.2",
    "prisma": "^5.6.0",
    "react": "^18",
    "react-dom": "^18",
    "react-popper": "^2.3.0",
    "rehype-add-classes": "^1.0.0",
    "remark-gfm": "^4.0.0",
    "sass": "^1.69.5",
    "type-fest": "^4.8.1"
  },
  "devDependencies": {
    "@activescott/eslint-config": "^2.3.6",
    "@types/jest": "^29.5.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv": "^16.4.7",
    "eslint": "^8",
    "eslint-config-next": "14.0.2",
    "eslint-plugin-unicorn": "^49.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "ts-node": "^10.9.1",
    "typescript": "^5"
  }
}
