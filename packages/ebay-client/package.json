{
  "name": "ebay-client",
  "type": "module",
  "version": "1.0.0",
  "module": "index.js",
  "files": [
    "dist/",
    "scripts/"
  ],
  "author": {
    "name": "Scott Willeke",
    "email": "scott@willeke.com",
    "url": "https://scott.willeke.com/"
  },
  "scripts": {
    "eslint": "eslint . --ext ts,tsx,js,jsx",
    "lint": "prettier -l \"{,!(node_modules)/**/}*.{ts,tsx,md,yml,json,html}\" && npm run eslint",
    "lint-fix": "prettier --write \"{,!(node_modules)/**/}*.{ts,tsx,md,yml,json,html}\" && npm run eslint --fix",
    "build:es": "rm -rfd ./dist/es; ./node_modules/.bin/tsc --project tsconfig.json",
    "build": "rm -rfd ./dist && npm run build:es",
    "clean": "rm -rf node_modules",
    "dev": "./node_modules/.bin/jest --watch",
    "test": "./node_modules/.bin/jest"
  },
  "keywords": [
    "ebay",
    "client",
    "api",
    "sdk",
    "rest",
    "ebay-api",
    "ebay-sdk",
    "ebay-rest",
    "ebay-client"
  ],
  "license": "AGPL-3.0",
  "devDependencies": {
    "@activescott/eslint-config": "^2.3.6",
    "@types/jest": "^29.5.10",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.9.4",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "irritable-iterable": "^1.3.1",
    "lodash-es": "^4.17.21",
    "type-fest": "^4.8.2"
  }
