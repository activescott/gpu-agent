{
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "dev": {
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^build", "^lint"]
    },
    "lint-fix": {
      "dependsOn": ["^lint-fix"]
    },
    "test": {
      "dependsOn": ["^test"]
    }
  }
}