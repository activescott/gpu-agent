{
  "name": "shopping-agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prep": "mkdir public/js; mkdir public/images; npm run prep-bootstrap && npm run prep-bootstrap-icons",
    "prep-bootstrap": "cp -f node_modules/bootstrap/dist/js/bootstrap.bundle.min.js public/js/",
    "prep-bootstrap-icons": "cp -f node_modules/bootstrap-icons/bootstrap-icons.svg public/images/",
    "predev": "npm run prep",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint-next": "next lint",
    "eslint": "eslint . --ext ts,tsx,js,jsx",
    "lint": "prettier -l \"{,!(node_modules)/**/}*.{ts,tsx,md,yml,json,html}\" && npm run eslint",
    "lint-fix": "prettier --write \"{,!(node_modules)/**/}*.{ts,tsx,md,yml,json,html}\" && npm run eslint -- --fix"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "@next/mdx": "^14.0.2",
    "@types/mdx": "^2.0.10",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.1",
    "next": "14.0.2",
    "react": "^18",
    "react-dom": "^18",
    "type-fest": "^4.8.1"
  },
  "devDependencies": {
    "@activescott/eslint-config": "^2.3.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.0.2",
    "eslint-plugin-unicorn": "^49.0.0",
    "typescript": "^5"
  }
}
