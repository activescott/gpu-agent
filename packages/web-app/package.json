{
  "name": "shopping-agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prep": "mkdir public/images; npm run prep-bootstrap-icons",
    "prep-bootstrap-icons": "cp -f ${npm_config_local_prefix}/node_modules/bootstrap-icons/bootstrap-icons.svg public/images/",
    "predev": "npm run prep",
    "dev": "DEBUG=shopping-agent:*,-shopping-agent:shop:listingFilters:debug next dev --turbopack",
    "//preinstall": "cd ../../packages/ebay-client && npm i && npm run build",
    "gen-sitemap": "npx ts-node src/scripts/gen-sitemap.ts",
    "prebuild": "npm run prep",
    "build": "DEBUG=shopping-agent:*,-shopping-agent:shop:listingFilters:* next build",
    "clean": "rm -rf node_modules; rm -rf .next; rm -rf .turbo",
    "start": "DEBUG=shopping-agent:*,ebay-client:* next start",
    "eslint": "eslint . --ext ts,tsx,js,jsx",
    "lint": "prettier -l \"{,!(node_modules)/**/}*.{ts,tsx,md,yml,json,html}\" && npm run eslint",
    "lint-fix": "prettier --write \"{,!(node_modules)/**/}*.{ts,tsx,md,yml,json,html}\" && npm run eslint -- --fix",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run prisma-generate",
    "watch": "DEBUG=shopping-agent:* jest --watch",
    "test": "DEBUG=shopping-agent:* jest",
    "prisma-validate": "npx prisma validate",
    "//prisma-generate": "generates the types for the prisma client",
    "prisma-generate": "npx prisma generate",
    "prisma-dev": "npx prisma migrate dev",
    "prisma-reset": "npx prisma migrate reset",
    "prisma-nuke": "npm run prisma-validate && rm -rf prisma/migrations && npm run prisma-dev && npm run prisma-reset",
    "prisma-seed": "npx prisma db seed"
  },
  "prisma": {
    "//seed": "seeds the database with bootstrap or test data. See https://www.prisma.io/docs/guides/migrate/seed-database.",
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@activescott/diag": "^2.0.2",
    "@activescott/ebay-client": "*",
    "@activescott/filter-items": "*",
    "@floating-ui/react": "^0.27.16",
    "@mdx-js/loader": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "@next/mdx": "15.5.5",
    "@popperjs/core": "^2.11.8",
    "@prisma/client": "^6.2.0",
    "@types/lodash": "^4.14.202",
    "@types/mdx": "^2.0.10",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.1",
    "canvas": "^3.2.0",
    "chart.js": "^4.5.1",
    "chartjs-node-canvas": "^5.0.0",
    "classnames": "^2.5.1",
    "common-tags": "^1.8.2",
    "glob": "^10.3.10",
    "irritable-iterable": "^1.4.0",
    "lodash": "^4.17.21",
    "next": "^15.5.7",
    "next-mdx-remote": "^5.0.0",
    "posthog-js": "^1.298.1",
    "prisma": "^6.2.0",
    "prom-client": "^15.1.3",
    "react": "^19",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19",
    "rehype-class-names": "^2.0.0",
    "remark-gfm": "^4.0.0",
    "sass": "^1.69.5",
    "styled-jsx": "^5.1.6",
    "type-fest": "^4.8.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@activescott/eslint-config": "^2.3.6",
    "@types/common-tags": "^1.8.4",
    "@types/jest": "^29.5.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv": "^16.4.7",
    "eslint": "^8",
    "eslint-config-next": "15.5.5",
    "eslint-plugin-unicorn": "^49.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "ts-node": "^10.9.1",
    "tsx": "^4.19.2",
    "typescript": "^5",
    "yaml": "^2.6.1"
  },
  "optionalDependencies": {
    "@next/swc-darwin-arm64": "^14.2.26",
    "@next/swc-darwin-x64": "^14.2.26",
    "@next/swc-linux-arm64-gnu": "^14.2.26",
    "@next/swc-linux-x64-gnu": "^14.2.26"
  }
}
